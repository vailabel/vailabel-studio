{
  "name": "vailabel",
  "version": "1.0.11",
  "description": "image labeling",
  "private": true,
  "authors": {
    "name": "Vichea Nath",
    "email": "nathvichea1@gmail.com"
  },
  "scripts": {
    "clean": "npx git-clear-branch",
    "format": "turbo run format",
    "commit": "cz",
    "prepare": "husky install",
    "test": "turbo run test",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "studio": "turbo run dev --filter=@vailabel/studio  --filter=@vailabel/api",
    "desktop": "turbo run dev --filter=vailabel-desktop --filter=@vailabel/studio",
    "release": "turbo run release --filter=@vailabel/studio --filter=vailabel-desktop",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "changeset": "changeset",
    "changeset:patch": "changeset add --empty",
    "changeset:minor": "changeset add --empty",
    "changeset:major": "changeset add --empty",
    "version": "changeset version",
    "release:patch": "yarn changeset:patch && yarn version && git push --follow-tags",
    "release:minor": "yarn changeset:minor && yarn version && git push --follow-tags",
    "release:major": "yarn changeset:major && yarn version && git push --follow-tags",
    "auto-release": "node scripts/auto-release.js",
    "release:auto": "yarn auto-release",
    "release:simple": "node scripts/simple-release.js",
    "release:now": "yarn release:simple"
  },
  "keywords": [],
  "workspaces": [
    "apps/*"
  ],
  "author": {
    "name": "Vichea Nath",
    "email": "nathvichea1@gmail.com"
  },
  "license": "GPL-3.0-only",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/jest": "^30.0.0",
    "@types/semver": "^7.7.0",
    "changesets": "^1.0.2",
    "commitizen": "^4.3.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "turbo": "^2.5.6"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "electron": "36.2.1"
  }
}
